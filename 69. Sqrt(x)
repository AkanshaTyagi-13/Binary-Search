class Solution {
    public int mySqrt(int x) {
        int s=1,e=x,ans=1;
        if(x==0) return 0;
        while(s<=e){
            int mid=s+(e-s)/2;
            if((long)mid*mid==(long)x) return mid;
            if((long)mid*mid<(long)x){
                ans=mid;
                s=mid+1;
            }
            else e=mid-1;
        }
        return ans;
    }
}
