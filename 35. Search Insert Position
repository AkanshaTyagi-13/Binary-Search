class Solution {
    public int searchInsert(int[] nums, int target) {
        int s=0,e=nums.length-1,ans=Integer.MAX_VALUE;
        while(s<=e){
            int mid=s+(e-s)/2;
            if(nums[mid]>=target){
                ans=Math.min(ans,mid);
                e=mid-1;
            }
            else{
                s=mid+1;
            }
        }
        return ans==Integer.MAX_VALUE?nums.length:ans;
    }
}
