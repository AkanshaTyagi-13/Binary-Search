class Solution {
    public int findMin(int[] nums) {
        int s=0, e=nums.length-1, len=nums.length;
        while(s<=e){
            if(len==1){
                return nums[0];
            }
            int mid=s+(e-s)/2;
            int next=(mid+1)%len;
            int prev=(mid+len-1)%len;
            if(nums[mid]<nums[next]&&nums[mid]<nums[prev]){
                return nums[mid];
            }
            else if(nums[mid]>nums[e]){
                s=mid+1;
            }
            else{
               e=mid-1; 
            }
        }
        return -1;
    }
}
